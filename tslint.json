{
  "extends": ["tslint:recommended", "tslint-config-prettier", "tslint-react"],
  "rules": {
    "no-console": [false],
    "object-literal-sort-keys": false,
    "jsx-no-multiline-js": false,
    "interface-name": false,
    "interface-over-type-literal": false
  },
  "linterOptions": {
    "exclude": [
      "dist"
    ]
  }
}
